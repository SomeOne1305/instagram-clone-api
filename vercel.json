{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "src/main.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],

  "buildCommand": "npm run build"
}
